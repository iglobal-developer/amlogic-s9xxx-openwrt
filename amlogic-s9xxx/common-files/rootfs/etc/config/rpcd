config rpcd
        option socket '/var/run/ubus/ubus.sock'
        option timeout '30'

config login
        option username 'root'
        option password '$p$root'
        list read '*'
        list write '*'

config login
        option username 'admin'
        option timeout '300'
        option password '$p$nobody'
        list read 'allow-full-uci-access'
        list read 'luci-app-mwan3'
        list read 'luci-app-statistics'
        list read 'luci-app-vproxy-config'
        list read 'luci-base'
        list read 'luci-base-network-status'
        list read 'luci-mod-status-channel_analysis'
        list read 'luci-mod-status-index'
        list read 'luci-mod-status-realtime'
        list read 'luci-mod-system-reboot'
        list read 'unauthenticated'
        list read 'vsocks-app-vproxy'
        list write 'allow-full-uci-access'
        list write 'luci-app-mwan3'
        list write 'luci-app-statistics'
        list write 'luci-app-vproxy-config'
        list write 'luci-mod-status-realtime'
        list write 'luci-mod-system-reboot'
        list write 'vsocks-app-vproxy'